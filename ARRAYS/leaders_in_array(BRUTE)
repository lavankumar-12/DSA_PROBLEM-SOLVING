vector<int> leaders_in_array(vector<int>& a)
{
    int n=a.size();
    vector<int> ans;
    for(int i=0;i<n;i++)
    {
        bool leader=true;
        for(int j=i+1;j<n;j++)
        {
            if(a[i]<a[j])
            {
                leader=false;
                break;
            }
        }
        if(leader)
            ans.push_back(a[i]);
    }
    return ans;
}