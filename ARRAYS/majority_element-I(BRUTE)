int majority_element_n_2(vector<int>& a)
{
    int n=a.size();
    for(int i=0;i<n;i++)
    {
        int count=1;
        for(int j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            {
                count++;
            }
        }
        if(count>(n/2))
        {
            return a[i];
        }
    }
    return -1;
}