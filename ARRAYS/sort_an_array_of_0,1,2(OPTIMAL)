void sort_an_array_of_0,1,2(vector<int>& a)
{
    int n=a.size();
    int low=0;
    int mid=0;
    int high=n-1;
    while(mid<=high)
    {
        if(a[mid]==0)
        {
            swap(a[low],a[mid]);
            low++;
            mid++;
        }
        else if(a[mid]==1)
        {
            mid++;
        }
        else
        {
            swap(a[mid],a[high]);
            high--;
        }
    }
    return;
}