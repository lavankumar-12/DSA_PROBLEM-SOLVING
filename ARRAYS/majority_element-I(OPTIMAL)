int majority_element_n_2(vector<int>& a)
{
    int n=a.size();
    int count=0;
    int ele=0;
    for(int i=0;i<n;i++)
    {
        if(count==0)
        {
            ele=a[i];
        }
        if(a[i]==ele)
        {
            count++;
        }
        else
        {
            count--;
        }
    }
    int cnt=0;
    for(int i=0;i<a.size();i++)
    {
        if(a[i]==ele)
            cnt++;
    }
    if(cnt>a.size()/2)
        return ele;
    return -1;
}